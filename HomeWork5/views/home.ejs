<h1><%=title%></h1>

<table border="1">
    <caption>Investors Table</caption>
<tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Year</th>
</tr>
<% for(var i=0; i < inventors.length; i++) { %>
   <tr>
     <td><%= inventors[i].first %></td>
     <td><%= inventors[i].last %></td>
     <td><%= inventors[i].year %></td>
   </tr>
<% } %>
</table>